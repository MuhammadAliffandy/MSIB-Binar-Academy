{
    "openapi": "3.0.3",
    "info": {
        "title": "Binar Car Documentation API",
        "version": "1.0.11"
    },
    "externalDocs": {
        "description": "Find out more about Swagger",
        "url": "http://swagger.io"
    },
    "servers": [
        {
        "url": "http://localhost:5000/"
        }
    ],
    "tags": [
        {
            "name": "cars",
            "description": "cars API"
        },
        {
            "name": "users",
            "description": "users API"
        }
    ],
    "paths": {
        "/cars": {
            "get": {
                "tags": [
                    "cars"
                ],
                "summary": "view all car data",
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "responses": {
                    "200": {
                    }
                }
            },
            "post": {
                "tags": [
                    "cars"
                ],
                "summary": "create a new car ",
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "requestBody": {
                        "content": {
                            "multipart/form-data": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "car-image": {
                                            "type": "string",
                                            "format": "binary",
                                            "description": "car image type file"
                                        },
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "name": {
                                                    "type": "string"
                                                },
                                                "rentPerDay": {
                                                    "type": "number"
                                                },
                                                "size": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "responses": {
                        "201": {
                            "description": "Mobil baru berhasil dibuat"
                        }
                    }
                }
            },
        "/cars/{id}": {
                "get": {
                    "tags": [
                        "cars"
                    ],
                    "summary": "Mendapatkan detail mobil berdasarkan ID",
                    "parameters": [
                        {
                            "name": "id",
                            "in": "path",
                            "required": true,
                            "schema": {
                            "type": "string"
                            }
                        }
                    ],
                    "responses": {
                        "200": {
                            "description": "Detail mobil berhasil didapatkan"
                        }
                    }
                },
                "put": {
                    "tags": [
                        "cars"
                    ],
                    "summary": "Memperbarui mobil berdasarkan ID",
                    "parameters": [
                        {
                            "name": "id",
                            "in": "path",
                            "required": true,
                            "schema": {
                            "type": "string"
                            }
                        }
                    ],
                    "requestBody": {
                        "content": {
                            "multipart/form-data": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "car-image": {
                                            "type": "string",
                                            "format": "binary",
                                            "description": "Gambar mobil (jenis file)"
                                        },
                                        "data": {
                                            "type": "object",
                                            "properties": {
                                                "name": {
                                                    "type": "string"
                                                },
                                                "rentPerDay": {
                                                    "type": "number"
                                                },
                                                "size": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "responses": {
                        "200": {
                            "description": "Mobil berhasil diperbarui"
                        }
                    }
                },
                "delete": {
                    "tags": [
                        "cars"
                    ],
                    "summary": "Memperbarui mobil berdasarkan ID",
                    "parameters": [
                        {
                            "name": "id",
                            "in": "path",
                            "required": true,
                            "schema": {
                            "type": "string"
                            }
                        }
                    ],
                    "responses": {
                        "200": {
                            "description": "Mobil berhasil diperbarui"
                        }
                    }
                }
        
            },
        "/users":{
            "get": {
                "tags": [
                    "users"
                ],
                "summary": "Mendapatkan daftar mobil",
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Daftar mobil berhasil didapatkan"
                    }
                }
            }
        },
        "/users/current-user":{
            "get": {
                "tags": [
                    "users"
                ],
                "summary": "Mendapatkan daftar mobil",
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Daftar mobil berhasil didapatkan"
                    }
                }
            }
        },
        "/users/register":{
            "post": {
                "tags": [
                    "users"
                ],
                "summary": "Membuat mobil baru",
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "requestBody": {
                    "content": {
                        "requestBody": {
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "object",
                                        "properties": {
                                            "name": {
                                                "type": "string"
                                            },
                                            "phone": {
                                                "type": "string"
                                            },
                                            "address": {
                                                "type": "string"
                                            },
                                            "email": {
                                                "type": "string"
                                            },
                                            "password": {
                                                "type": "string"
                                            }
                                        },
                                        "required": ["name","email","password"]
                                    }
                                }
                            }
                            }
                        }
                    },
                    "responses": {
                        "201": {
                            "description": "Mobil baru berhasil dibuat"
                        }
                    }
                }
        },
        "/users/register-admin":{
            "post": {
                "tags": [
                    "users"
                ],
                "summary": "Membuat mobil baru",
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "requestBody": {
                    "content": {
                        "requestBody": {
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "object",
                                        "properties": {
                                            "name": {
                                                "type": "string"
                                            },
                                            "phone": {
                                                "type": "string"
                                            },
                                            "address": {
                                                "type": "string"
                                            },
                                            "email": {
                                                "type": "string"
                                            },
                                            "password": {
                                                "type": "string"
                                            }
                                        },
                                        "required": ["name","email","password"]
                                    }
                                }
                            }
                            }
                        }
                    },
                    "responses": {
                        "201": {
                            "description": "Mobil baru berhasil dibuat"
                        }
                    }
                }
        },
        "/users/auth":{
            "post": {
                "tags": [
                    "users"
                ],
                "summary": "Membuat mobil baru",
                "requestBody": {
                    "content": {
                        "requestBody": {
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "object",
                                        "properties": {
                                            "email": {
                                                "type": "string"
                                            },
                                            "password": {
                                                "type": "string"
                                            }
                                        },
                                        "required": ["email","password"]
                                    }
                                }
                            }
                            }
                        }
                    },
                    "responses": {
                        "201": {
                            "description": "Mobil baru berhasil dibuat"
                        }
                    }
                }
        },
        "/users/auth/google":{

        },
        "/users/auth/facebook":{

        },
        "/users/logout":{
            "post": {
                "tags": [
                    "users"
                ],
                "summary": "Membuat mobil baru",
                "responses": {
                    "201": {
                        "description": "Mobil baru berhasil dibuat"
                    }
                }
                }
        }
    },
    "components": {
        "securitySchemes": {
            "BearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        }
    }    
}